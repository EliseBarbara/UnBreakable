<Controls:MetroWindow x:Class="Vista.Crear_Contrato"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:behaviours="clr-namespace:MahApps.Metro.Behaviours;assembly=MahApps.Metro"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" 
                      
        xmlns:local="clr-namespace:Vista"
        mc:Ignorable="d"
        Title="OnBreak - Administrar Contrato" Height="583.835" Width="985.128" WindowStartupLocation="CenterScreen" FontWeight="Bold" WindowTitleBrush="Black" Icon="Imagenes/white_logo_dark_background.jpg">
    <Grid Margin="0,0,-8,-1" Background="#FF84D9E8" Height="798" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="-0.133"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="986.4"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#CC119EDA" HorizontalAlignment="Left" Height="29" Margin="769,3,0,0" Stroke="#FFA0A0A0" VerticalAlignment="Top" Width="211"/>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="109,26,0,0" Stroke="#CC119EDA" VerticalAlignment="Top" Width="640" Fill="#FF84D9E8"/>
        <Label x:Name="label_Copy1" Content="Número:" HorizontalAlignment="Left" Height="36" Margin="142,34,0,0" VerticalAlignment="Top" Width="118"/>
        <TextBox x:Name="txtNumero" Controls:TextBoxHelper.Watermark ="Ingrese Número Contrato" HorizontalAlignment="Left" Height="26" Margin="244,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166"/>
        <Button x:Name="btnBuscarContrato" Content="Buscar" HorizontalAlignment="Left" Margin="479,36,0,0" VerticalAlignment="Top" Width="74" Height="28" FontWeight="Normal" Click="btnBuscarContrato_Click_2" />
        <Button x:Name="btnListadoNum" Content="?" HorizontalAlignment="Left" Margin="426,36,0,0" VerticalAlignment="Top" Width="38" Height="26" Click="btnListadoNum_Click"/>
        <Label x:Name="label_Copy2" Content="Nombre:" HorizontalAlignment="Left" Height="36" Margin="142,140,0,0" VerticalAlignment="Top" Width="76"/>
        <Label x:Name="label_Copy4" Content="Rut Cliente:" HorizontalAlignment="Left" Height="36" Margin="142,83,0,0" VerticalAlignment="Top" Width="118"/>
        <TextBox x:Name="txtBuscarCliente" Controls:TextBoxHelper.Watermark ="Ingrese Rut" HorizontalAlignment="Left" Height="26" Margin="244,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166" />
        <Button x:Name="btnListadoCliente" Content="?" HorizontalAlignment="Left" Margin="428,81,0,0" VerticalAlignment="Top" Width="40" Height="27" RenderTransformOrigin="0.5,0.5" Click="btnListadoCliente_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.673"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="281,197,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="0"/>
        <Button x:Name="btnCrear" Content="Crear" HorizontalAlignment="Left" Margin="149,501,0,0" VerticalAlignment="Top" Width="73" Click="btnCrear_Click" FontWeight="Normal" RenderTransformOrigin="0.5,0.5" Height="28">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.157"/>
                    <TranslateTransform Y="0.069"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btnModificar" Content="Modificar" HorizontalAlignment="Left" Margin="438,503,0,0" VerticalAlignment="Top" Width="82" FontWeight="Normal" RenderTransformOrigin="0.5,0.5" Height="28" Click="btnModificar_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.157"/>
                    <TranslateTransform X="-0.001" Y="-0.417"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btnLimpiar" Content="Limpiar" HorizontalAlignment="Left" Margin="306,501,0,0" VerticalAlignment="Top" Width="75" FontWeight="Normal" Click="btnLimpiar_Click" RenderTransformOrigin="0.5,0.5" Height="30">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.157"/>
                    <TranslateTransform X="0.001" Y="0.433"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="button" Content="Cerrar" HorizontalAlignment="Left" Margin="842,501,0,0" VerticalAlignment="Top" Width="76" FontWeight="Normal" Height="28" Click="button_Click"/>
        <Label x:Name="label6" Content="Tipo Evento:" HorizontalAlignment="Left" Margin="137,279,0,0" VerticalAlignment="Top" Height="26" Width="82"/>
        <ComboBox x:Name="cboTipo" HorizontalAlignment="Left" Margin="244,279,0,0" VerticalAlignment="Top" Width="145" AutomationProperties.IsOffscreenBehavior="Onscreen" Height="26"/>
        <Label x:Name="label4" Content="Dirección:" HorizontalAlignment="Left" Margin="137,233,0,0" VerticalAlignment="Top" Height="26" Width="69"/>
        <TextBox x:Name="txtDireccion" Controls:TextBoxHelper.Watermark ="Ingrese dirección" HorizontalAlignment="Left" Height="26" Margin="244,237,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="292" />
        <Label x:Name="label5_Copy" Content="N° Asistentes:" HorizontalAlignment="Left" Margin="137,323,0,0" VerticalAlignment="Top" Height="26" Width="88"/>
        <TextBox x:Name="txtNumeroAsistentes" HorizontalAlignment="Left" Height="26" Margin="245,323,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="36" TextChanged="txtNumeroAsistentes_TextChanged_1"/>
        <Label x:Name="label7" Content="Observaciones:" HorizontalAlignment="Left" Margin="135,375,0,0" VerticalAlignment="Top" Height="26" Width="95"/>
        <TextBox x:Name="txtObservaciones" HorizontalAlignment="Left" Height="74" Margin="244,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="292"/>
        <Label x:Name="label9" Content="Buscar" HorizontalAlignment="Left" Margin="109,0,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="Black" Height="34" Width="67" FontSize="16" FontFamily="Segoe UI Historic" FontWeight="Bold"/>
        <Label x:Name="label5" Content="Contrato Vigente:" HorizontalAlignment="Left" Margin="769,3,0,0" VerticalAlignment="Top" Height="26" Width="118"/>
        <RadioButton x:Name="rbSi" Content="Sí" HorizontalAlignment="Left" Margin="884,10,0,0" VerticalAlignment="Top" IsChecked="True" Height="18" Width="34" GroupName="vigente"/>
        <RadioButton x:Name="rbNo" Content="No" HorizontalAlignment="Left" Margin="934,10,0,0" VerticalAlignment="Top" Height="18" Width="52" GroupName="vigente"/>
        <Button x:Name="btnCliente" Content="Buscar" HorizontalAlignment="Left" Margin="479,79,0,0" VerticalAlignment="Top" Width="75" FontWeight="Normal" Click="btnCliente_Click" Height="29"/>
        <Label x:Name="lblNombreCliente" Content="" HorizontalAlignment="Left" Margin="244,140,0,0" VerticalAlignment="Top" Width="310"/>
        <Label x:Name="label9_Copy" Content="Datos Evento" HorizontalAlignment="Left" Margin="109,191,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="Black" Height="34" Width="121" FontSize="16" FontFamily="Segoe UI Historic" FontWeight="Bold"/>
        <Label x:Name="label5_Copy2" Content="N° Personal Adicional:" HorizontalAlignment="Left" Margin="359,323,0,0" VerticalAlignment="Top" Height="26" Width="136"/>
        <TextBox x:Name="txtPersonalAdicional" HorizontalAlignment="Left" Height="23" Margin="522,323,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" TextChanged="txtPersonalAdicional_TextChanged_1"/>
        <Label x:Name="label" Content="SubTotal :" HorizontalAlignment="Left" Margin="675,237,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Asistentes :" HorizontalAlignment="Left" Margin="675,268,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy3" Content="Personal Adicional :" HorizontalAlignment="Left" Margin="675,299,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy5" HorizontalAlignment="Left" Margin="675,323,0,0" VerticalAlignment="Top" Width="282" Height="25">
            <Separator HorizontalAlignment="Left" Height="16" Margin="0" VerticalAlignment="Top" Width="567"/>
        </Label>
        <Label x:Name="label9_Copy1" Content="Valor Evento" HorizontalAlignment="Left" Margin="674,191,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="Black" Height="34" Width="121" FontSize="16" FontFamily="Segoe UI Historic" FontWeight="Bold"/>
        <Label x:Name="label_Copy6" Content="Valor Total :" HorizontalAlignment="Left" Margin="675,348,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblValorBase" Content="" HorizontalAlignment="Left" Margin="864,237,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblAsistentes" Content="" HorizontalAlignment="Left" Margin="864,268,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblPersonalAdicional" Content="" HorizontalAlignment="Left" Margin="864,299,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblTotal" Content="" HorizontalAlignment="Left" Margin="864,348,0,0" VerticalAlignment="Top" Width="81"/>
    </Grid>
</Controls:MetroWindow>
